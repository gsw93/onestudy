<!-- JAVASCRIPTS -->

<script type="text/javascript">

    var x=<%-board.location.coordinates[1]%>;
    var y=<%-board.location.coordinates[0]%>;
    function myMap() {
        var mapCanvas = document.getElementById("gMap");
        var myCenter = new google.maps.LatLng(x,y);
        var marker = new google.maps.Marker({position:myCenter});
        var mapOptions = {
            center: myCenter,
            zoom: 14
        };
        var map = new google.maps.Map(mapCanvas, mapOptions);
        marker.setMap(map);

        google.maps.event.addListener(marker,'click',function() {
            map.setZoom(17);
            map.setCenter(marker.getPosition());
        });
    }
    //본인글 본인이 후기 작성 금지 & 참여학생외 금지
    $('form#replyAction').on('submit', function (e) {
      <%if(authUser.id==board.id){%>
        alert("본인은 작성하실 수 없습니다.");
        document.getElementById('commentcontents').value ="";
        return false;
      <%}%>
      <%if(!board.studentList.length){%>
        alert("참여학생이 아닌 학생은 작성하실 수 없습니다.");
        document.getElementById('commentcontents').value ="";
        return false;
      <%}else{%>
      <%for(var i=0;i<board.studentList.length;i++){
        if(!(authUser.id==board.studentList[i].email)){%>
          alert("참여학생이 아닌 학생은 작성하실 수 없습니다.");
          document.getElementById('commentcontents').value ="";
          return false;
        <%}}}%>
    });
    //본인글 본인 스터디 학생금지
    $('form#studentaction').on('submit', function (e) {
      <%if(authUser.id==board.id){%>
        alert("본인은 작성하실 수 없습니다.");
        document.getElementById('selfIntroduction').value ="";
        return false;
      <%}%>
    });


    $(function(){
      $('.text').froalaEditor({
        key: 'bA2A8B8D7cD3F3C2C1G3A4C4B6C2D1vdbhA-21D-17eukD1wzB1C-22j==',
        height: 240,
        language:'ko',
        fontFamily: {
          "'Nanum Gothic',sans-serif":'나눔고딕',
