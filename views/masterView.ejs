<%for(var n=0;n< board.studentList.length;n++){
if(board.stop[0].statue && (board.studentList[n].email==authUser.id)){%>
<!-- 수업 중단 학생 -->
<div class="modal fade" role="dialog" id="stopmaster" style="top: 10%;">
    <div class="modal-dialog signup-content" >
            <br><br>
        <form action="/process/stopstudy?id=<%=board._id%>" method="post" class="form-horizontal">
