<section class="store-coupon section">
    <!-- Container Start -->
    <div class="container">
        <div class="row">
            <div class="col-md-10 col-lg-11" style="margin:auto;">
                <!-- Block -->
                <div class="block">
                    <div class="media">
                        <!-- Store Icon -->
                        <div class="row" style="width:100%;margin:auto;">

                            <div class="col-sm-12 col-md-5 col-lg-2 image-content d-flex align-self-center">
                              <img src="<%- board.masterphoto %>"  alt="master_profile">
                            </div>

                            <div class="col-sm-12 col-md-7 col-lg-7 media-body">
                                <!-- <form action="/process/addboard" method="post"> -->
                                <!-- Heading -->
                                <span class="headerSign"><%=board.title %></span>
                                <!-- Description -->
                                <div class="row">
                                  <div class="col-lg-6 form-group">
                                    <label class="darktext">지역</label>
                                    <span style=""><%= board.regionShort %></span>
                                  </div>
                                  <div class="col-lg-6 form-group">
                                    <label class="darktext">카테고리</label>
                                    <% for(var i=0; i < board.category.length;i++){ %>
                                    <span style=""><%= board.category[i] %></span>
                                    <% } %>
                                  </div>
                                  <div class="col-lg-6 form-group">
                                    <label class="darktext">모집기한</label>
                                    <span id="deadline"></span>
                                    <script>
                                      var i = new Date('<%- board.deadline %>');
                                      document.getElementById('deadline').innerHTML = date_format(i);
                                      function date_format(dateValue){
                                        var dd = dateValue.getDate();
                                        var mm = dateValue.getMonth()+1;
                                        var yyyy = dateValue.getFullYear();

                                        if(dd<10){
                                          dd = '0'+dd;
                                        }
                                        if(mm<10){
                                          mm = '0'+mm;
                                        }
                                        return yyyy+'-'+mm+'-'+dd;
                                      }
                                    </script>
                                  </div>
                                  <div class="col-lg-6 form-group">
                                    <label class="darktext">모집인원</label>
                                      <label class="control-label" style="font-size: 14px;" for="minNum">최소</label>
                                      <span>&nbsp;<%=board.minNum%>명</span>
                                      <label class="control-label" style="font-size: 14px;" for="maxNum">최대</label>
                                      <span>&nbsp;<%=board.maxNum%>명</span>
                                      <!-- <label class="control-labe" style="font-size: 14px;" for="currentNum">현재</label>
                                      <span>&nbsp;<%=board.currentNum%>명</span> -->
                                  </div>
                                  <div class="col-lg-6 form-group">
                                    <label for="studyTerm" class="darktext">스터디 기간</label>
                                    <span>&nbsp;<%=board.studyTerm%>주</span>
                                  </div>
                                  <div class="col-lg-6 form-group">
                                    <label for="price" class="darktext">수업료<small style="color:gray;">(총)</small></label>
                                    <span style="font-size:22px;font-weight: 700;color:#ff751a;">&nbsp;주 <%=board.studynum%>회 <%=board.price%>만원</span>
                                  </div>
                                </div>
                            </div>

                            <div class="col-md-12 col-lg-2">
                                <!--핸드폰 인증 기능 개발되면 !authPhone을 authPhone으로 바꿔 인증된 사람만 할 수 있게 수정해야함 !!-->
                                <%if(authUser.phoneAuthCheck){%>
                                <!-- 작성자 수정 -->
                                <%if(authUser.id==board.id){%>
                                <button type="button" class="btn btn-transparent" onclick="location.href='/ModifywriteMaster?id=<%=board._id%>'" style="width:100%;">수정하기</button>
                                <%if(!board.stop[0].statue){%>
                                <a href="" data-toggle="modal" data-target="#stop"  class="stop">모집중단</a>
                                <%}} else{ var flag=0; %>
                                  <%if(authUser.mystudy.length){
                                    for(var a=0; a< authUser.mystudy.length; a++){%>
                                  <%if(board._id==authUser.mystudy[a].studyid){ flag=1;}}}
                                  if(board.currentNum==board.maxNum){flag=1;}%>
                                  <%if(!flag){%>
                                  <button type="button" class="btn btn-transparent" data-toggle="modal" data-target="#join" id="joinbtn" style="width:100%;">참여 신청하기</button>
                                  <%}}} else {%>
                                <a href="/studentapply" style="width:100%;" class="btn btn-transparent">참여 신청하기</a>
                                <%}%>
                                <script type="text/javascript">
                                  var today = new Date();
                                  var term = new Date('<%- board.deadline %>');
                                  term.setDate(term.getDate()+(<%=board.studyTerm%>*7));
                                  if(today>term){
                                    document.getElementById('joinbtn').style.display = "none";
                                  }
                                </script>
                            </div>
                        </div>
                    </div>
                  </div>
                <article style="margin:0;padding:0px;">
                    <div class="post-slider owl-carousel owl-theme" style="width: 100%;">
                        <div class="item"><img src="<%=board.path[0]%>" style ="width: 100%;height: 350px;" alt="Slider-Img-01"></div>
                        <%if(board.path.length > 1){
                        for(var s=1;s< board.path.length;s++){%>
                        <div class="item"><a href=""><img src="<%=board.path[s]%>" style="width: 100%;height: 350px;" alt="Slider-Img-02"></a></div>
                        <%}}%>
                    </div>
                </article>

                <div style="margin-top:50px;">
                  <div class="editor_cnt">
                      <%if(authUser.id==board.id){%>
                      <div class="col-lg-2 title">
                          <h5>참여 인원</h5>
                      </div>

                      <div class="col-lg-10 block">
                        <table class="table applypeople">
                          <thead>
                            <tr>
                              <th>이름</th>
                              <th>상태</th>
                              <th>연락처</th>
                              <th>크래딧</th>
                            </tr>
                          </thead>
                          <tbody>
                            <%if(board.studentList.length){%>
                                <%for (var i=0; i< board.studentList.length; i++){%>
                                  <tr>
                                    <td class="name"></td>
                                    <script type="text/javascript">
                                      var name = "<%=board.studentList[i].name%>";
                                      document.getElementsByClassName('name')[<%=i%>].innerHTML = name.substr(0,1) +"XX";
                                    </script>
                                    <%if(!board.studentList[i].statue){%>
                                      <td>신청</td>
                                      <td>010-XXXX-XXXX</td>
                                      <td><%=(2000+(board.price*200))%></td>
                                    <%} else{%>
                                      <td>결제완료</td>
                                      <td><%=board.studentList[i].phone%></td>
                                      <td style="text-decoration: line-through;color:#808080;"><%=(2000+(board.price*200))%></td>
                                    <%}%>
                                  </tr>
                                <%}%>
                            <%}%>
                          </tbody>
                        </table>
                        <button class="btn btn-primary col-sm-3 paybtn" onclick="location.href='/payment?id=<%=board._id%>'">결제하기 ></button>
                      </div>
                      <%}%>

                    <div class="col-lg-2 title">
                        <h5>Master 소개</h5>
                    </div>
                    <div class="col-lg-10 block">
                      <%- board.masterInfo %>
                    </div>
                  </div>
                  <div class="editor_cnt">
                    <div class="col-lg-2 title">
                        <h5>Study 소개</h5>
                    </div>
                    <div class="col-lg-10 block">
                      <%- board.studyInfo %>
                    </div>
                  </div>
                  <div class="editor_cnt">
                    <div class="col-lg-2 title">
                        <h5>스터디 장소</h5>
                    </div>
                    <div id="gMap" class="col-lg-10 block" style="height:350px;"></div>
                  </div>
                  <div class="editor_cnt">
                    <div class="col-lg-2 title">
                        <h6>마스터에 대한 후기</h6>
                    </div>
                    <div class="col-lg-10 block" style="padding: 25px 0px 30px 0">
                      <div class="review_star">
                        <span>스터디평점</span>
                        <span><%= board.reviewstar+'.0' %></span>
                        <span>
                          <% var temp=0;var s; for(s=0;s< board.reviewstar;s++){ %>
                          <i class="fas fa-star" style="color:#ff751a;"></i><% } %>
                          <% for(s;s< 5;s++){%><i class="fas fa-star" style="color:#e6e6e6;"></i><%}%></span>
                      </div>

                        <% if(board.comments.length>0){ var commentsList = board.comments;%>
                          <% for (var i =0;i< commentsList.length;i++){ %>
                      <div class="row comment" style="width:100%;">
                        <div class="col-lg-2 commentimg">
                          <img src="<%= authUser.photo %>" style="height: 55px;width:55px;margin:auto;display: block;border-radius:50%;" alt="">
                        </div>
                        <div class="col-lg-10 commentcnt">
                          <table style="width:100%;">
                            <form id="commentdelete" action="/process/deleteComment" method="post">
                            <tr>
                              <td><%=commentsList[i].author%></td>
                              <td id="comment_date[<%=i%>]" style="font-size:13px;width:50px;"></td>
                              <script>
                              var i = new Date('<%- commentsList[i].comment_date %>');
                              document.getElementById('comment_date[<%=i%>]').innerHTML = date_format2(i);
                                function date_format2(dateValue){
                                  var dd = dateValue.getDate();
                                  var mm = dateValue.getMonth()+1;
                                  var yyyy = dateValue.getFullYear();
                                  var hh = dateValue.getHours();
                                  var mimi = dateValue.getMinutes();
                                  if(dd<10){
                                    dd = '0'+dd;
                                  }
                                  if(mm<10){
                                    mm = '0'+mm;
                                  }
                                  if(hh<10){
                                    hh = '0'+hh;
                                  }
                                  if(mimi<10){
                                    mimi = '0'+mimi;
                                  }
                                  return yyyy+'.'+mm+'.'+dd+' - '+hh+':'+mimi;
                                }
                              </script>
                              <td>
                                <% var a; for(a=0;a< commentsList[i].star_rating;a++){ %>
                                <i class="fas fa-star" style="color:#ff751a;">
                                <%} for(a;a< 5; a++){ %>
                                <i class="fas fa-star" style="color:#e6e6e6;">
                                  <% } temp=temp+commentsList[i].star_rating;%>
                              </td>
                            </tr>
                            <tr>
                              <input type="hidden" name="commentcnt" value="<%=commentsList[i].contents%>">
                              <td colspan="3" style="font-size:18px;color:gray;width:100%;" id="comment_cnt[<%=i%>]"><%=commentsList[i].contents%></td>
                            </tr>
                            <%if(commentsList[i].author==authUser.name){%>
                            <tr>
                              <input type="hidden" name="Id" value="<%=board._id%>">
                              <td style="font-size:11px;color:#ccc;">
                                <button type="submit" name="button" style="border:none;background:none;font-size:11px;margin-right:5px;">수정</button> /
                                <button type="submit" name="button" style="border:none;background:none;font-size:11px;">삭제</button>
                              </td>
                            </tr>
                            <%}%>
                            </form>
                          </table>
                        </div>
                      </div>
                      <%}}%>
                      <form id="replyAction" action="/process/comment" method="post">
                      <div class="rating-stars" style="background-color:#e6e6e6;margin:20px;padding:25px;text-align:center;">
                        <ul id='stars'>
                          <li class='star' title='Poor' data-value='1'>
                            <i class="fas fa-star"></i>
                          </li>
                          <li class='star' title='Fair' data-value='2'>
                            <i class="fas fa-star"></i>
                          </li>
                          <li class='star' title='Good' data-value='3'>
                            <i class="fas fa-star"></i>
                          </li>
                          <li class='star' title='Excellent' data-value='4'>
                            <i class="fas fa-star"></i>
                          </li>
                          <li class='star' title='Perfect' data-value='5'>
                            <i class="fas fa-star"></i>
                          </li>
                        </ul>
                        <div id="rating_point" value="0.0">0.0</div>
                        <input type="hidden" id="hidden_point" name="star_rating" value="">
                        <input type="hidden" id="reviewstar" name="reviewstar" value="">
                      </div>
                      <%for(var n=0;n< board.studentList.length;n++){
                      if(!board.stop[0].statue){%>
                      <div class="comment_write" style="margin:20px;">
                        <input type="hidden" name="replyId" id="replyId" value="<%=board._id%>">
                        <textarea name="contents" id="commentcontents" class="form-control" rows="2"
                                  style="display: inline;width:80%;float:left;" placeholder="감사합니다."  /required></textarea>
                        <button type="submit" name="button" class="btn" style="background-color:#ff751a;color:#fff;float:left;width:15%;height:78px;">전송</button>
                      </div>
                      <%}}%>
                      </form>
                    </div>
                  </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Container End -->
</section>

<!-- 참여하기 모달 -->
<div class="modal fade" role="dialog" id="join" style="top: 10%;">
    <div class="modal-dialog signup-content" >
            <br><br>
        <form action="/process/participateStudy?id=<%=board._id%>" id="studentaction" method="post" class="form-horizontal">
            <div class="form-group" style="margin:auto;">
                <div class="real_content">
                    <h2 style="text-align: center;">간단한 자기소개</h2>
                    <p style="text-align:center; color:gray;">마스터에게 하고 싶은 말을 써주세요.</p>
                    <div class="login-set">
                        <textarea name="selfIntroduction" rows="8" id="selfIntroduction" style="width: 100%;"></textarea>
                    </div>
                    <div class="login-set">
                        <button type="submit" class="form-control btn btn-warning" class="sign-in2">스터디 참여하기</button>
                    </div>
                    <input type="hidden" name="replyId" id="replyId2" value="<%=board._id%>">
                </div>
            </div>
        </form>
        <br><br>
    </div>
</div>

